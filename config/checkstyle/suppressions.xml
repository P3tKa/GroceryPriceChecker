<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <suppress files="[\\/]build[\\/]" checks="[a-zA-Z0-9]*"/>
    <suppress checks="MissingJavadocMethod"></suppress>
    <suppress checks="MissingJavadocType"></suppress>
    <suppress checks="OperatorWrap"></suppress>
    <suppress checks="VariableDeclarationUsageDistance"></suppress>
    <suppress checks="AbbreviationAsWordInName"></suppress>
    <suppress checks="OverloadMethodsDeclarationOrder"></suppress>
    <suppress checks="PackageName"></suppress>
</suppressions>
